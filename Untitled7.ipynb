   "source": [
    "l = [1,2,3,4,5,6,7,8,9] \n",
    "\n",
    "even, odd= 0, 0\n",
    "\n",
    "for i in l: \n",
    "\n",
    "    if i % 2 == 0: \n",
    "\n",
    "        odd += 1\n",
    "\n",
    "    else: \n",
    "\n",
    "        even+= 1          \n",
    "\n",
    "print(\"Number of even number : \", even) \n",
    "\n",
    "print(\"Number of odd number : \", odd)"
   ]
